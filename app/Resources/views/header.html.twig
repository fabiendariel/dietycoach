  <!-- Main Header -->
  <header class="main-header">

    <!-- Header Navbar -->
    <nav class="navbar navbar-static-top" role="navigation">
        <div class="navbar-header">
            <span class="navbar-brand">Dietycoach</span>
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                <i class="fa fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
              {% if app.user.hasRole is defined and (app.user.hasRole('ROLE_SUPER_ADMIN') or app.user.hasRole('ROLE_ADMIN')) %}
                <ul class="nav navbar-nav">
                  <li class=""><a href="{{ path('app_administration_codes') }}"><i class="fa fa-list"></i><span> Codes</span></a></li>
                </ul>
              {% endif %}
              {% if app.user.hasRole is defined and (app.user.hasRole('ROLE_SUPER_ADMIN') or app.user.hasRole('ROLE_ADMIN')) %}
                <ul class="nav navbar-nav">
                  <li class=""><a href="{{ path('app_administration_coachs', {'page': 1})}}"><i class="fa fa-briefcase"></i><span> Coachs</span></a></li>
                </ul>
              {% endif %}
              {% if app.user.hasRole is defined and (app.user.hasRole('ROLE_SUPER_ADMIN')) %}
                <ul class="nav navbar-nav">
                  <li class=""><a href="{{ path('app_administration_utilisateurs', {'page': 1})}}"><i class="fa fa-users"></i><span> Utilisateurs</span></a></li>
                </ul>
              {% endif %}
            </div>
        </div>
      <!-- Navbar Right Menu -->
      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
       
          <li class="user-menu">
              {% if app.user.username is defined %}
                 <a>{{ app.user.username }}</a>
              {% endif %}
          </li>
            {% if app.user.hasRole is defined %}
            <li class="user-menu">
            <a href="{{ path('fos_user_security_logout')}}"><i class="fa fa-sign-out"></i> Déconnexion</span></a>
                {% endif %}
          </li>

        </ul>
      </div>
    </nav>
  </header>
{% for flash_message in app.session.flashBag.get('notice') %}
<div id="message" style="position: absolute;top: 20px;right: 20px; z-index: 5000; width: 40%;">
    <div class="alert alert-info alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <h4><i class="icon fa fa-info"></i>Information</h4>
                    {{ flash_message }}
    </div>
</div>
{% endfor %}
{% for flash_message in app.session.flashBag.get('error') %}
<div id="message" style="position: absolute;top: 20px;right: 20px; z-index: 5000; width: 40%;">
    <div class="alert alert-error alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <h4><i class="icon fa fa-ban"></i>Erreur</h4>
                    {{ flash_message }}
    </div>
</div>
{% endfor %}
{% for flash_message in app.session.flashBag.get('success') %}
<div id="message" style="position: absolute;top: 20px;right: 20px; z-index: 5000; width: 40%;">
    <div class="alert alert-success alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                    <h4><i class="icon fa fa-check"></i>Success</h4>
                    {{ flash_message }}
    </div>
</div>
{% endfor %}